project adatftpd is

   for Languages use ("Ada");
   for Source_Dirs use ("src/**");
   for Object_Dir use "obj";
   for Exec_Dir use "test";
   for Main use ("adatftpd.adb");

   package Builder is
      for Executable ("adatftpd.adb") use "adatftpd";
   end Builder;

   package Compiler is
      for Switches ("ada") use ("-gnatVimopr", "-gnatwku");
   end Compiler;

end adatftpd;

